{
    "name": "ga",
    "version": "0.0.1-demo",
    "description": "NM-CNPM",
    "scripts": {
        "preinstall": "node ./preinstall-script.js",
        "setup": "watchman watch-del-all && rm -rf yarn.lock && rm -rf node_modules/ && cd ./client && yarn install && cd ../server && yarn install",
        "server:dev": "yarn --cwd ./server run dev",
        "client:dev": "yarn --cwd ./client run dev ",
        "dev": "npx concurrently \"yarn run server:dev\" \"yarn run client:dev\"",
        "start": "yarn run setup && yarn run dev",
        "format": "npx prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "keywords": [],
    "author": "hidang <yellowstudio.vn@gmail.com>",
    "license": "MIT",
    "main": "index.js",
    "dependencies": {
        "axios": "^1.4.0",
        "concurrently": "^8.2.0",
        "react": "^18.2.0",
        "react-admin": "^4.11.3",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.12",
        "@vitejs/plugin-react": "^4.0.0",
        "prettier": "^2.8.8",
        "vite-plugin-babel-macros": "^1.0.6"
    }
}
